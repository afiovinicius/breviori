{
  "version": 2,
  "buildCommand": "pip install --no-cache-dir poetry",
  "installCommand": "poetry install --no-dev -no-interaction --no-ansi",
  "devCommand": "gunicorn -k uvicorn.workers.UvicornWorker app.main:app --bind 0.0.0.0:8080",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
